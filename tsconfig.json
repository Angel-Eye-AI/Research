{
    "compilerOptions": {
      "target": "ES2020",
      "module": "ES2020",
      "moduleResolution": "node",
      "strict": true,
      "sourceMap": true,
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "outDir": "./data/dist",
      "rootDir": "./data"
    },
    "include": [
      "data/**/*.ts"
    ],
    "exclude": [
      "node_modules"
    ]
  }
  